/* ===== DPO Issue Page ‚Äî Premium ===== */

.dpo-container {
  width: 100%;
  height: 100vh;
  overflow: hidden;
  display: flex;
  flex-direction: column;
}

.dpo-container .dpo-content {
  padding: 16px 70px;
  position: relative;
  flex: 1;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  animation: pageEnter 0.4s ease both;
}

@keyframes pageEnter {
  from {
    opacity: 0;
    transform: translateY(10px);
  }

  to {
    opacity: 1;
    transform: translateY(0);
  }
}

/* ‚îÄ‚îÄ Title ‚îÄ‚îÄ */
.dpo-container .dpo-page-title {
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
  margin-bottom: 20px;
}

.dpo-container .dpo-page-title h1 {
  font-size: 2rem;
  font-weight: 700;
  margin: 0;
  color: var(--color-text, #1f2937);
}

/* ‚îÄ‚îÄ Upload Icon ‚îÄ‚îÄ */
.dpo-upload-icon {
  font-size: 35px !important;
  margin-top: 40px;
  cursor: pointer;
  color: var(--color-text-secondary, #6b7280);
  transition: color var(--transition-fast, 0.15s ease),
    transform var(--transition-fast, 0.15s ease);
}

.dpo-upload-icon:hover {
  color: var(--color-primary-dark, #1d4ed8);
  transform: scale(1.08);
}

/* ‚îÄ‚îÄ Scroll area ‚îÄ‚îÄ */
.dpo-tab-scroll-area {
  flex: 1;
  min-height: 0;
  overflow-y: auto;
  display: flex;
  flex-direction: column;
  padding-right: 10px;
  scrollbar-width: thin;
  scrollbar-color: #cbd5e1 transparent;
}

.dpo-tab-scroll-area::-webkit-scrollbar {
  width: 6px;
}

.dpo-tab-scroll-area::-webkit-scrollbar-track {
  background: transparent;
}

.dpo-tab-scroll-area::-webkit-scrollbar-thumb {
  background-color: #cbd5e1;
  border-radius: 20px;
}

/* ‚îÄ‚îÄ Tabs wrapper ‚îÄ‚îÄ */
.dpo-tabs-wrapper {
  position: relative;
  margin-top: 10px;
}

.dpo-tabs-add-icon {
  position: absolute;
  right: -2px;
  top: 60%;
  transform: translateY(-50%);
  cursor: pointer;
  color: var(--color-text-secondary, #6b7280);
  transition: color var(--transition-fast, 0.15s ease),
    transform var(--transition-fast, 0.15s ease);
}

.dpo-tabs-add-icon:hover {
  color: var(--color-primary, #2563eb);
  transform: translateY(-50%) scale(1.15);
}

.dpo-no-issues {
  text-align: center;
  margin-top: 40px;
  padding: 20px;
  background: #f3f4f6;
  border-radius: var(--radius-lg, 14px);
}

.dpo-loading-text {
  opacity: 0.7;
}

/* ‚îÄ‚îÄ MODAL ‚îÄ‚îÄ */
.dpo-assign-overlay {
  position: fixed;
  inset: 0;
  z-index: 9999;
  display: flex;
  align-items: center;
  justify-content: center;
  animation: modalFadeIn 0.2s ease;
}

@keyframes modalFadeIn {
  from {
    opacity: 0;
  }

  to {
    opacity: 1;
  }
}

.dpo-assign-backdrop {
  position: absolute;
  inset: 0;
  background: rgba(0, 0, 0, 0.35);
  backdrop-filter: blur(6px);
