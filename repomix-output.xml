}

@keyframes fadeInUp {
  from {
    opacity: 0;
    transform: translateY(14px);
  }

  to {
    opacity: 1;
    transform: translateY(0);
  }
}

.empty-state-compact {
  width: 100%;
  min-height: 100%;
  margin-top: 0;
  padding: 0;
}

.empty-state-illustration {
  width: clamp(160px, 24vh, 220px);
  height: clamp(160px, 24vh, 220px);
  margin: 0 auto 8px;
  display: block;
  animation: floatIllustration 4s ease-in-out infinite;
}

@keyframes floatIllustration {

