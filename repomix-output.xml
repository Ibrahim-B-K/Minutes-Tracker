/* ===== DPO Minutes Page ‚Äî Premium ===== */

.dpo-container .dpo-page-title {
  font-size: 2rem;
  font-weight: 700;
  margin-bottom: 20px;
  color: var(--color-text, #1f2937);
}

.dpo-container .dpo-minutes-page-content {
  padding: 16px 70px;
  flex: 1;
  min-height: 0;
  display: flex;
  flex-direction: column;
  overflow: hidden;
  animation: pageEnter 0.4s ease both;
}

@keyframes pageEnter {
  from {
    opacity: 0;
    transform: translateY(10px);
  }

  to {
    opacity: 1;
    transform: translateY(0);
  }
}

.dpo-container .dpo-minutes-body {
  flex: 1;
  min-height: 0;
  overflow-y: auto;
}

/* ‚îÄ‚îÄ Error ‚îÄ‚îÄ */
.dpo-container .dpo-error-message {
  background: #fef2f2;
  color: #dc2626;
  padding: 12px 16px;
  border-radius: var(--radius-md, 10px);
  margin-bottom: 20px;
  border-left: 4px solid #dc2626;
  font-size: 0.9rem;
  font-weight: 500;
}

/* ‚îÄ‚îÄ Grid ‚îÄ‚îÄ */
.dpo-container .dpo-minutes-grid {
  display: grid;
  grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));
  gap: 18px;
}

/* ‚îÄ‚îÄ Card ‚îÄ‚îÄ */
.dpo-container .dpo-minutes-card {
  background: var(--color-surface, #fff);
  border-radius: var(--radius-lg, 14px);
  box-shadow: var(--shadow-sm, 0 2px 8px rgba(0, 0, 0, 0.06));
  overflow: hidden;
  border: 1px solid var(--color-border, #e2e8f0);
  transition: transform var(--transition-base, 0.25s ease),
    box-shadow var(--transition-base, 0.25s ease),
    border-color var(--transition-base, 0.25s ease);
}

.dpo-container .dpo-minutes-card:hover {
  transform: translateY(-3px);
  box-shadow: var(--shadow-md, 0 4px 16px rgba(0, 0, 0, 0.08));
  border-color: var(--color-border-hover, #cbd5e1);
}

/* ‚îÄ‚îÄ Card header ‚îÄ‚îÄ */
.dpo-container .dpo-card-header {
  display: flex;
  align-items: center;
  gap: 12px;
  padding: 16px;
  background: linear-gradient(135deg, #f8fafc, #f1f5f9);
  border-bottom: 1px solid var(--color-border, #e2e8f0);
}

.dpo-container .dpo-pdf-icon {
  font-size: 32px;
  color: #dc2626;
  flex-shrink: 0;
}

.dpo-container .dpo-file-name {
  margin: 0;
  font-size: 0.95rem;
  font-weight: 600;
  color: var(--color-text, #1f2937);
  word-break: break-word;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}

/* ‚îÄ‚îÄ Card Body ‚îÄ‚îÄ */
.dpo-container .dpo-card-body {
  padding: 16px;
}

.dpo-container .dpo-info-item {
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
  margin-bottom: 10px;
  gap: 8px;
  font-size: 0.88rem;
}

.dpo-container .dpo-info-item:last-child {
  margin-bottom: 0;
}

.dpo-container .dpo-info-item .dpo-label {
  font-weight: 600;
  color: var(--color-text-secondary, #6b7280);
  min-width: 115px;
  font-size: 0.82rem;
  text-transform: uppercase;
  letter-spacing: 0.03em;
}

.dpo-container .dpo-info-item .dpo-value {
  color: var(--color-text, #1f2937);
  text-align: right;
  flex: 1;
  word-break: break-word;
}

/* ‚îÄ‚îÄ Card Actions ‚îÄ‚îÄ */
.dpo-container .dpo-card-actions {
  display: flex;
  gap: 16px;
  padding: 12px 16px;
  background: #f9fafb;
  border-top: 1px solid var(--color-border, #e2e8f0);
}

.dpo-container .dpo-minutes-view-btn,
.dpo-container .dpo-minutes-download-btn,
.dpo-container .dpo-minutes-delete-btn {
  background: none;
  border: none;
  padding: 4px;
  cursor: pointer;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 22px;
  border-radius: var(--radius-sm, 6px);
  transition: transform var(--transition-fast, 0.15s ease),
    color var(--transition-fast, 0.15s ease),
    background var(--transition-fast, 0.15s ease);
  flex-shrink: 0;
}

.dpo-container .dpo-minutes-view-btn {
  color: var(--color-primary, #2563eb);
}

.dpo-container .dpo-minutes-view-btn:hover {
  color: var(--color-primary-dark, #1d4ed8);
  transform: scale(1.15);
  background: rgba(37, 99, 235, 0.06);
}

.dpo-container .dpo-minutes-download-btn {
  color: var(--color-success, #10b981);
}

.dpo-container .dpo-minutes-download-btn:hover {
  color: #059669;
  transform: scale(1.15);
  background: rgba(16, 185, 129, 0.06);
}

.dpo-container .dpo-minutes-delete-btn {
  color: var(--color-danger-dark, #dc2626);
}

.dpo-container .dpo-minutes-delete-btn:hover:not(:disabled) {
  color: #b91c1c;
  transform: scale(1.15);
  background: rgba(220, 38, 38, 0.06);
}

.dpo-container .dpo-minutes-delete-btn:disabled {
  color: var(--color-text-muted, #9ca3af);
  cursor: not-allowed;
  opacity: 0.7;
}

/* ‚îÄ‚îÄ Responsive ‚îÄ‚îÄ */
@media (max-width: 768px) {
  .dpo-container .dpo-minutes-grid {
    grid-template-columns: 1fr;
  }

  .dpo-container .dpo-page-title {
    font-size: 1.5rem;
  }

  .dpo-container .dpo-info-item {
    flex-direction: column;
    align-items: flex-start;
  }

  .dpo-container .dpo-info-item .dpo-label {
    min-width: unset;
  }

  .dpo-container .dpo-info-item .dpo-value {
    text-align: left;
  }
}

.dpo-container .dpo-minutes-body>.empty-state-card,
.dpo-container .dpo-minutes-body>.loading-state {
  margin-top: 0;
  min-height: 100%;
}
