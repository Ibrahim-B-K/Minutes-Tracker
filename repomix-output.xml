  animation: fadeInCard 0.35s ease forwards;
  transition: transform var(--transition-base, 0.25s ease), box-shadow var(--transition-base, 0.25s ease);
}

.department-issue-card:hover {
  transform: translateY(-2px);
  box-shadow: var(--shadow-md, 0 4px 16px rgba(0, 0, 0, 0.08));
}

.department-issue-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 14px;
}

.department-issue-id {
  font-weight: 700;
  font-size: 1rem;
  color: var(--color-text, #1f2937);
}

.department-status {
  font-size: 0.78rem;
  padding: 4px 12px;
  border-radius: var(--radius-full, 9999px);
  font-weight: 600;
  text-transform: capitalize;
  letter-spacing: 0.02em;
}

.department-status.department-pending {
  background: #fef9c3;
  color: #854d0e;
}

.department-status.department-overdue {
  background: #fee2e2;
  color: #b91c1c;
}

.department-status.department-submitted {
  background: #dcfce7;
  color: #166534;
}

.department-issue-body p {
  margin: 4px 0;
  font-size: 0.88rem;
  color: #333;
}

.department-issue-row {
  display: flex;
  align-items: flex-start;
  margin: 5px 0;
  font-size: 0.88rem;
  color: #333;
}

.department-issue-row .department-label {
  width: 120px;
  font-weight: 600;
  color: var(--color-text-secondary, #6b7280);
  position: relative;
  padding-right: 8px;
  font-size: 0.82rem;
  text-transform: uppercase;
  letter-spacing: 0.03em;
}

.department-issue-row .department-label::after {
  content: ":";
  position: absolute;
  right: 0;
}

.department-issue-row .department-value {
  flex: 1;
  padding-left: 12px;
  margin-top: 0;
  color: var(--color-text, #1f2937);
}

.department-overdue-text {
  color: #b91c1c !important;
  font-weight: 600;
}

/* =========================
   METRICS ROW
========================= */

.department-issue-metrics {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: 12px;
  margin-top: 20px;
  padding-top: 16px;
  border-top: 1px dashed var(--color-border, #e2e8f0);
}

.department-metric {
  display: flex;
  flex-direction: column;
  gap: 6px;
}

.department-metric-label {
  font-size: 0.72rem;
  font-weight: 700;
  color: var(--color-text-secondary, #6b7280);
  text-transform: uppercase;
  letter-spacing: 0.05em;
}

.department-metric-value {
  font-size: 0.88rem;
  font-weight: 600;
  color: var(--color-text, #1f2937);
  display: flex;
  align-items: center;
}

.department-dept-tag {
  background: #f1f5f9;
  padding: 4px 10px;
  border-radius: 6px;
  font-family: inherit;
  font-size: 0.8rem;
  display: inline-block;
  width: fit-content;
  border: 1px solid #e2e8f0;
}

.department-priority-high {
  color: #dc2626 !important;
}

.department-priority-medium {
  color: #ca8a04 !important;
}

.department-priority-low {
  color: #16a34a !important;
}

@media (max-width: 600px) {
  .department-issue-metrics {
    grid-template-columns: 1fr;
    gap: 16px;
  }
}

.department-footer-line {
  height: 1px;
  background: var(--color-border, #e2e8f0);
  margin-top: 16px;
}

.department-issue-footer {
  margin-top: 12px;
}

.department-response-received {
  display: flex;
  flex-direction: column;
  background: #ecfdf5;
  padding: 10px 12px;
  border-radius: var(--radius-md, 10px);
  gap: 4px;
  align-items: flex-start;
  border: 1px solid #bbf7d0;
}

.department-res {
  margin: 0;
  font-size: 0.88rem;
  line-height: 1.4;
  color: #333;
  text-align: left;
  width: 100%;
}

.department-res strong {
  color: #000;
  margin-right: 4px;
}

.department-res-button {
  align-self: flex-end;
  background: linear-gradient(135deg, #16a34a, #15803d);
  color: white;
  border: none;
  padding: 6px 14px;
  border-radius: var(--radius-sm, 6px);
  cursor: pointer;
  font-size: 0.82rem;
  font-weight: 600;
  font-family: inherit;
  box-shadow: 0 2px 6px rgba(22, 163, 74, 0.2);
  transition: transform var(--transition-fast, 0.15s ease), box-shadow var(--transition-fast, 0.15s ease);
}

.department-res-button:hover {
  transform: translateY(-1px);
  box-shadow: 0 3px 10px rgba(22, 163, 74, 0.3);
}

.department-submit-btn {
  position: absolute;
  right: 12px;
  bottom: 12px;
  color: #fff;
  border: none;
  padding: 8px 18px;
  border-radius: var(--radius-md, 10px);
  cursor: pointer;
  font-weight: 600;
  font-family: inherit;
  box-shadow: 0 3px 10px rgba(0, 0, 0, 0.15);
  transition: transform var(--transition-fast, 0.15s ease), box-shadow var(--transition-fast, 0.15s ease);
}

.department-pending-btn {
  background: linear-gradient(135deg, #d7b838, #c9a526);
}

.department-pending-btn:hover {
  transform: translateY(-1px) scale(1.02);
}

.department-overdue-btn {
  background: linear-gradient(135deg, #b91c1c, #991b1b);
}

.department-overdue-btn:hover {
  transform: translateY(-1px) scale(1.02);
}

@keyframes fadeInCard {
  from {
    opacity: 0;
    transform: translateY(6px);
  }

  to {
    opacity: 1;
    transform: translateY(0);
  }
}
</file>

<file path="client/src/components/Department/DepartmentResponseModal.css">
/* ===== Department Response Modal  Premium ===== */

.department-modal-overlay {
  position: fixed;
  inset: 0;
  background: rgba(0, 0, 0, 0.35);
  backdrop-filter: blur(6px);
  -webkit-backdrop-filter: blur(6px);
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 999;
  animation: deptModalFade 0.2s ease;
}

@keyframes deptModalFade {
  from {
    opacity: 0;
  }

  to {
    opacity: 1;
  }
}

.department-modal-content {
  position: relative;
  background: var(--color-surface, #fff);
  padding: 40px 28px 24px;
  border-radius: var(--radius-lg, 14px);
  width: 420px;
  box-shadow: 0 24px 60px rgba(0, 0, 0, 0.18);
  animation: deptModalSlide 0.25s ease;
  font-family: var(--font-family, 'Inter', sans-serif);
  border: 1px solid var(--color-border, #e2e8f0);
}

@keyframes deptModalSlide {
  from {
    transform: scale(0.96) translateY(8px);
    opacity: 0;
  }

  to {
    transform: scale(1) translateY(0);
    opacity: 1;
  }
}

.department-modal-content::-webkit-scrollbar {
  display: none;
}

.department-close-btn {
  position: absolute;
  top: 14px;
  right: 16px;
  background: none;
  border: none;
  font-size: 22px;
  color: var(--color-text-muted, #9ca3af);
  cursor: pointer;
  transition: color var(--transition-fast, 0.15s ease);
}

.department-close-btn:hover {
  color: var(--color-text, #1f2937);
}

.department-issue-title {
  font-size: 1.25rem;
  font-weight: 700;
  text-align: center;
  color: var(--color-text, #1f2937);
  margin-bottom: 20px;
}

.department-modal-fields {
  display: flex;
  flex-direction: column;
  gap: 14px;
  margin-bottom: 20px;
}

.department-modal-row {
  display: flex;
  flex-direction: column;
  gap: 5px;
}

.department-modal-row label {
  font-size: 12px;
  color: var(--color-text-secondary, #6b7280);
  font-weight: 600;
  text-transform: uppercase;
  letter-spacing: 0.03em;
}

.department-modal-row input,
.department-modal-row textarea {
  padding: 9px 12px;
  border: 1.5px solid var(--color-border, #e2e8f0);
  border-radius: var(--radius-md, 10px);
  font-size: 14px;
  font-family: inherit;
  transition: border-color var(--transition-base, 0.25s ease), box-shadow var(--transition-base, 0.25s ease);
  outline: none;
}

.department-modal-row input:focus,
.department-modal-row textarea:focus {
  border-color: var(--color-primary, #2563eb);
  box-shadow: 0 0 0 3px rgba(37, 99, 235, 0.12);
}

.department-modal-row input[readonly] {
  background: #f9fafb;
  color: var(--color-text-secondary, #6b7280);
  cursor: not-allowed;
}

.department-modal-row textarea {
  resize: none;
  height: 70px;
}

.department-modal-row input[type="file"] {
  border: none;
  font-size: 13px;
  padding: 0;
}

.department-modal-actions {
  display: flex;
  justify-content: flex-end;
  gap: 10px;
}

.department-modal-actions.department-single-btn {
  display: flex;
  justify-content: center;
}

.department-cancel-btn {
  background: #f3f4f6;
  border: none;
  padding: 9px 18px;
  border-radius: var(--radius-md, 10px);
  cursor: pointer;
  font-weight: 500;
  font-family: inherit;
  transition: background var(--transition-fast, 0.15s ease);
}

.department-cancel-btn:hover {
  background: #e5e7eb;
}

.department-submit-btn {
  background: linear-gradient(135deg, var(--color-primary, #2563eb), var(--color-primary-dark, #1d4ed8));
  color: white;
  border: none;
  padding: 9px 20px;
  border-radius: var(--radius-md, 10px);
  cursor: pointer;
  font-weight: 600;
  font-family: inherit;
  box-shadow: 0 2px 8px rgba(37, 99, 235, 0.25);
  transition: transform var(--transition-fast, 0.15s ease), box-shadow var(--transition-fast, 0.15s ease);
}

.department-submit-btn:hover {
  transform: translateY(-1px);
  box-shadow: 0 4px 14px rgba(37, 99, 235, 0.35);
}

/*  Upload area  */
.department-upload-area {
  border: 2px dashed var(--color-border-hover, #cbd5e1);
  border-radius: var(--radius-lg, 14px);
  padding: 20px;
  text-align: center;
  color: var(--color-text-secondary, #6b7280);
  background: #f9fafb;
  cursor: pointer;
  transition: border-color var(--transition-base, 0.25s ease), background var(--transition-base, 0.25s ease), color var(--transition-base, 0.25s ease);
}

.department-upload-area:hover {
  border-color: var(--color-primary, #2563eb);
  background: #eff6ff;
  color: #1e3a8a;
}

.department-file-name {
  font-size: 14px;
  color: var(--color-text, #1f2937);
  font-weight: 500;
  word-break: break-all;
}

.department-su {
  background: linear-gradient(135deg, var(--color-primary, #2563eb), var(--color-primary-dark, #1d4ed8));
  color: white;
  border: none;
  padding: 10px 18px;
  border-radius: var(--radius-md, 10px);
  cursor: pointer;
  font-weight: 600;
  font-family: inherit;
  box-shadow: 0 2px 8px rgba(37, 99, 235, 0.25);
  margin-left: auto;
  display: block;
  transition: transform var(--transition-fast, 0.15s ease), box-shadow var(--transition-fast, 0.15s ease);
}

.department-su:hover {
  transform: translateY(-1px);
  box-shadow: 0 4px 14px rgba(37, 99, 235, 0.35);
}
