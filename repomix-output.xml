<file path="client/src/components/DPO/UploadPage/DPOUploadForm.css">
/* ===== DPO Upload Form ‚Äî Premium ===== */

.dpo-upload-form {
  background: var(--color-surface, #fff);
  border-radius: var(--radius-lg, 14px);
  padding: 28px;
  box-shadow: var(--shadow-md, 0 4px 16px rgba(0, 0, 0, 0.08));
  max-width: 800px;
  margin: 24px auto 0;
  font-family: var(--font-family, 'Inter', sans-serif);
  border: 1px solid var(--color-border, #e2e8f0);
}

.dpo-upload-title {
  font-size: 1.4rem;
  font-weight: 700;
  color: var(--color-text, #1f2937);
  margin-bottom: 10px;
}

.dpo-field-label {
  display: block;
  font-size: 13px;
  font-weight: 600;
  color: var(--color-text-secondary, #6b7280);
  margin-top: 20px;
  margin-bottom: 6px;
  text-transform: uppercase;
  letter-spacing: 0.03em;
}

.dpo-date-input-wrapper input[type="text"] {
  width: 100%;
  padding: 10px 40px 10px 12px;
  border: 1.5px solid var(--color-border, #e2e8f0);
  border-radius: var(--radius-md, 10px);
  font-size: 14px;
  font-family: inherit;
  color: var(--color-text, #1f2937);
  outline: none;
  transition: border-color var(--transition-base, 0.25s ease), box-shadow var(--transition-base, 0.25s ease);
}

.dpo-date-input-wrapper input[type="text"]:focus {
  border-color: var(--color-primary, #2563eb);
  box-shadow: 0 0 0 3px rgba(37, 99, 235, 0.12);
}

.dpo-calendar-icon {
  position: absolute;
  right: 12px;
  cursor: pointer;
  font-size: 18px;
  color: var(--color-text-secondary, #6b7280);
  transition: color var(--transition-fast, 0.15s ease);
}

.dpo-calendar-icon:hover {
  color: var(--color-primary, #2563eb);
}

/* ‚îÄ‚îÄ Drag-drop area ‚îÄ‚îÄ */
.dpo-file-drop {
  border: 2px dashed var(--color-border-hover, #cbd5e1);
  border-radius: var(--radius-lg, 14px);
  height: 140px;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 10px;
  position: relative;
  background: #fafafa;
  transition: border-color var(--transition-base, 0.25s ease),
    background var(--transition-base, 0.25s ease);
}

.dpo-file-drop:hover {
  border-color: var(--color-primary-light, #3b82f6);
  background: rgba(37, 99, 235, 0.02);
}

.dpo-file-drop.dpo-drag-over {
  border-color: var(--color-primary, #2563eb);
  background: #eff6ff;
  border-style: solid;
}

.dpo-file-drop-inner {
  text-align: center;
  cursor: pointer;
}

.dpo-file-icon {
  font-size: 32px;
  margin-bottom: 8px;
  color: var(--color-primary-light, #3b82f6);
}

.dpo-file-text {
  font-size: 14px;
  color: var(--color-text-secondary, #6b7280);
}

.dpo-file-input {
  position: absolute;
  inset: 0;
  opacity: 0;
  cursor: pointer;
}

/* ‚îÄ‚îÄ Upload button ‚îÄ‚îÄ */
.dpo-up {
  display: block;
  width: 100%;
  margin-top: 25px;
  background: linear-gradient(135deg, var(--color-primary, #2563eb), var(--color-primary-dark, #1d4ed8));
  color: #fff;
  border: none;
  padding: 12px 0;
  font-size: 15px;
  font-weight: 600;
  font-family: inherit;
  border-radius: var(--radius-md, 10px);
  cursor: pointer;
  box-shadow: 0 4px 12px rgba(37, 99, 235, 0.25);
  transition: transform var(--transition-fast, 0.15s ease),
    box-shadow var(--transition-fast, 0.15s ease);
}

.dpo-up:hover {
  transform: translateY(-1px);
  box-shadow: 0 6px 18px rgba(37, 99, 235, 0.35);
}

.dpo-up:active {
  transform: translateY(0);
  box-shadow: 0 2px 8px rgba(37, 99, 235, 0.2);
}

.dpo-form-content {
  display: flex;
  flex-direction: column;
  gap: 10px;
}

/* ‚îÄ‚îÄ Loading card ‚îÄ‚îÄ */
.dpo-loading-card {
  background: var(--color-surface, #fff);
  border-radius: var(--radius-lg, 14px);
  padding: 28px;
  box-shadow: var(--shadow-md, 0 4px 16px rgba(0, 0, 0, 0.08));
  max-width: 550px;
  margin: 24px auto 0;
  font-family: var(--font-family, 'Inter', sans-serif);
  border: 1px solid var(--color-border, #e2e8f0);
}

/* ‚îÄ‚îÄ Extraction animation ‚îÄ‚îÄ */
.dpo-extract-wrapper {
  display: flex;
  flex-direction: column;
  align-items: center;
  gap: 14px;
  font-family: var(--font-family, system-ui, sans-serif);
}

.dpo-extract-wrapper h4 {
  color: var(--color-primary, #2563eb);
  margin: 0;
  text-align: center;
  font-weight: 700;
}

.dpo-document {
  position: relative;
  width: 220px;
  height: 240px;
  background: #f9fbff;
  border: 2px solid var(--color-primary-light, #3b82f6);
  border-radius: var(--radius-md, 10px);
  padding: 20px;
  overflow: hidden;
}

.dpo-document::after {
  content: "";
  position: absolute;
  bottom: 0;
  left: 0;
  height: 0%;
  width: 100%;
  background: rgba(37, 99, 235, 0.06);
  animation: fillDoc 4s ease-in-out infinite;
  z-index: 0;
}

.dpo-scan-line {
  position: absolute;
  top: 0;
  left: 0;
  height: 3px;
  width: 100%;
  background: linear-gradient(90deg, transparent, var(--color-primary, #2563eb), transparent);
  animation: scan 2s ease-in-out infinite;
  z-index: 2;
}

.dpo-field {
  height: 14px;
  background: #dbeafe;
  border-radius: 4px;
  margin-bottom: 14px;
  animation: highlight 2s ease-in-out infinite;
  position: relative;
  z-index: 1;
}

.dpo-field.dpo-short {
  width: 60%;
}

.dpo-field.dpo-medium {
  width: 80%;
}

.dpo-status {
  font-size: 14px;
  color: var(--color-primary, #2563eb);
  letter-spacing: 0.3px;
  text-align: center;
}

@keyframes scan {
  0% {
    top: 0;
    opacity: 0;
  }

  10% {
    opacity: 1;
  }

  50% {
    opacity: 1;
  }

  100% {
    top: 100%;
    opacity: 0;
  }
}

@keyframes fillDoc {
  0% {
    height: 0%;
  }

  100% {
    height: 100%;
  }
}

@keyframes highlight {

  0%,
  100% {
    opacity: 0.5;
  }

  50% {
    opacity: 1;
  }
}
</file>

<file path="client/src/pages/CollectorPage/NotificationPage.css">
/* Base layout */
.collector-container {
  width: 100%;
  height: 100vh;
  overflow: hidden;
  display: flex;
  flex-direction: column;
}

.collector-container .collector-content {
  padding: 10px 70px;
  flex: 1;
  min-height: 0;
  overflow: hidden;
  display: flex;
  flex-direction: column;
}

.collector-container h1 {
  font-size: 2rem;
  font-weight: 700;
  margin: 0 0 20px;
  color: #111;
}

/* Notification List */
.collector-container .collector-notification-list {
  background: #ffffff;
  border-radius: 12px;
  padding: 15px;
  box-shadow: 0 6px 16px rgba(0, 0, 0, 0.06);
  display: flex;
  flex-direction: column;
  gap: 10px;
  overflow-y: auto;
  flex: 1;
  min-height: 0;
}

/* Notification Card */
.collector-container .collector-notification-card {
  display: flex;
  align-items: flex-start;
  gap: 10px;
  border-left: 5px solid transparent;
  background: #f9fafb;
  padding: 8px 10px;
  border-radius: 10px;
  border: 1px solid #e5e7eb;
  transition: all 0.2s ease;
}

.collector-container .collector-notification-card:hover {
  transform: translateY(-2px);
  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.05);
}

.collector-container .collector-notification-card.collector-response {
  border-left-color: #3b82f6;
}

.collector-container .collector-notification-card.collector-deadline {
  border-left-color: #ef4444;
}

.collector-container .collector-notification-card.collector-assign {
  border-left-color: #22c55e;
}

/* Icon */
.collector-container .collector-notification-icon span {
  font-size: 1.6rem;
  display: flex;
  align-items: center;
  justify-content: center;
}

/* Text */
.collector-container .collector-notification-content h3 {
  font-size: 1rem;
  font-weight: 600;
  color: #111827;
  margin: 0 0 4px;
}

.collector-container .collector-notification-content p {
  font-size: 0.9rem;
  color: #4b5563;
  margin: 0;
}

.collector-container .collector-notification-content .collector-time {
  display: block;
  font-size: 0.8rem;
  color: #9ca3af;
  margin-top: 4px;
}

/* Empty state */
.collector-container .collector-no-notifications {
  text-align: center;
  padding: 50px 0;
  color: #6b7280;
  font-size: 0.95rem;
}

.collector-container .collector-notification-list > .empty-state-card,
.collector-container .collector-notification-list > .loading-state {
  margin-top: 0;
  min-height: 100%;
}

/* Responsive */
@media (max-width: 768px) {
  .collector-container .collector-content {
    padding: 15px 25px;
  }

  .collector-container .collector-notification-list {
    padding: 12px;
  }
}
</file>

