<file path="client/src/components/DPO/IssuePage/DPOIssueCard.css">
/* =========================
   ISSUE CARD  Premium
========================= */

.dpo-issue-card {
  position: relative;
  background: var(--color-surface, #fff);
  border-radius: var(--radius-lg, 14px);
  box-shadow: var(--shadow-sm, 0 2px 8px rgba(0, 0, 0, 0.06));
  padding: 20px;
  margin-top: 10px;
  border-left: 5px solid #facc15;
  border: 1px solid var(--color-border, #e2e8f0);
  border-left: 5px solid #facc15;
  opacity: 0;
  animation: fadeInCard 0.35s ease forwards;
  transition: transform var(--transition-base, 0.25s ease),
    box-shadow var(--transition-base, 0.25s ease);
}

.dpo-issue-card:hover {
  transform: translateY(-2px);
  box-shadow: var(--shadow-md, 0 4px 16px rgba(0, 0, 0, 0.08));
}

.dpo-issue-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 14px;
}

.dpo-issue-header-left {
  display: flex;
  align-items: center;
  gap: 8px;
  min-width: 0;
}

.dpo-issue-id {
  font-weight: 700;
  font-size: 1rem;
  color: var(--color-text, #1f2937);
  flex-shrink: 0;
}

.dpo-issue-minute-tag {
  font-size: 0.7rem;
  font-weight: 500;
  color: #6b7280;
  background: #f1f5f9;
  padding: 2px 8px;
  border-radius: 6px;
  border: 1px solid #e2e8f0;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  max-width: 200px;
}

.dpo-follow-up-badge {
  display: inline-flex;
  align-items: center;
  gap: 3px;
  font-size: 0.65rem;
  font-weight: 600;
  color: #2563eb;
  background: #eff6ff;
  padding: 2px 7px;
  border-radius: 4px;
  border: 1px solid #bfdbfe;
  white-space: nowrap;
}

.dpo-has-followups-badge {
  display: inline-flex;
  align-items: center;
  font-size: 0.65rem;
  font-weight: 600;
  color: #7c3aed;
  background: #f5f3ff;
  padding: 2px 7px;
  border-radius: 4px;
  border: 1px solid #ddd6fe;
  white-space: nowrap;
}

.dpo-status {
  font-size: 0.78rem;
  padding: 4px 12px;
  border-radius: var(--radius-full, 9999px);
  font-weight: 600;
  text-transform: capitalize;
  letter-spacing: 0.02em;
}

/* Status Colors */
.dpo-status.dpo-pending {
  background: #fef9c3;
  color: #854d0e;
}

.dpo-status.dpo-overdue {
  background: #fee2e2;
  color: #b91c1c;
}

.dpo-status.dpo-received {
  background: #dcfce7;
  color: #166534;
}

/* =========================
   BODY ROWS
========================= */

.dpo-issue-row {
  display: flex;
  align-items: flex-start;
  margin: 5px 0;
  font-size: 0.88rem;
  color: #333;
}

.dpo-issue-row .dpo-label {
  width: 120px;
  font-weight: 600;
  color: var(--color-text-secondary, #6b7280);
  position: relative;
  padding-right: 8px;
  font-size: 0.82rem;
  text-transform: uppercase;
  letter-spacing: 0.03em;
}

.dpo-issue-row .dpo-label::after {
  content: ":";
  position: absolute;
  right: 0;
}

.dpo-issue-row .dpo-value {
  flex: 1;
  padding-left: 12px;
  margin-top: 0;
  color: var(--color-text, #1f2937);
}

.dpo-overdue-text {
  color: #b91c1c !important;
  font-weight: 600;
}

/* =========================
   METRICS ROW
========================= */

.dpo-issue-metrics {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: 12px;
  margin-top: 20px;
  padding-top: 16px;
  border-top: 1px dashed var(--color-border, #e2e8f0);
}

.dpo-metric {
  display: flex;
  flex-direction: column;
  gap: 6px;
}

.dpo-metric-label {
  font-size: 0.72rem;
  font-weight: 700;
  color: var(--color-text-secondary, #6b7280);
  text-transform: uppercase;
  letter-spacing: 0.05em;
}

.dpo-metric-value {
  font-size: 0.88rem;
  font-weight: 600;
  color: var(--color-text, #1f2937);
  display: flex;
  align-items: center;
}

.dpo-dept-tag {
  background: #f1f5f9;
  padding: 4px 10px;
  border-radius: 6px;
  font-family: inherit;
  font-size: 0.8rem;
  display: inline-block;
  width: fit-content;
  border: 1px solid #e2e8f0;
}

.dpo-priority-high {
  color: #dc2626 !important;
}

.dpo-priority-medium {
  color: #ca8a04 !important;
}

.dpo-priority-low {
  color: #16a34a !important;
}

@media (max-width: 600px) {
  .dpo-issue-metrics {
    grid-template-columns: 1fr;
    gap: 16px;
  }
}

/* =========================
   FOOTER
========================= */

.dpo-footer-line {
  height: 1px;
  background: var(--color-border, #e2e8f0);
  margin: 16px 0 10px;
}

.dpo-issue-footer {
  padding-bottom: 40px;
}

/* =========================
   RESPONSES
========================= */

.dpo-response-received {
  display: flex;
  flex-direction: column;
  background-color: #ecfdf5;
  padding: 10px 12px;
  border-radius: var(--radius-md, 10px);
  margin: 0 0 8px;
  gap: 4px;
  align-items: flex-start;
  border: 1px solid #bbf7d0;
}

.dpo-res {
  margin: 0;
  font-size: 0.88rem;
  line-height: 1.4;
  color: #333;
  width: 100%;
}

.dpo-res strong {
  color: #000;
  margin-right: 4px;
}

.dpo-res-button {
  align-self: flex-end;
  background: linear-gradient(135deg, #16a34a, #15803d);
  color: white;
  border: none;
  padding: 6px 14px;
  border-radius: var(--radius-sm, 6px);
  cursor: pointer;
  font-size: 0.82rem;
  font-weight: 600;
  font-family: inherit;
  box-shadow: 0 2px 6px rgba(22, 163, 74, 0.2);
  transition: transform var(--transition-fast, 0.15s ease),
    box-shadow var(--transition-fast, 0.15s ease);
}

.dpo-res-button:hover {
  transform: translateY(-1px);
  box-shadow: 0 3px 10px rgba(22, 163, 74, 0.3);
}

/* =========================
   LOG BUTTON
========================= */

.dpo-log-btn {
  position: absolute;
  bottom: 12px;
  right: 15px;
  background: linear-gradient(135deg, var(--color-primary, #2563eb), var(--color-primary-dark, #1d4ed8));
  color: white;
  border: none;
  padding: 6px 16px;
  border-radius: var(--radius-sm, 6px);
  cursor: pointer;
  font-size: 13px;
  font-weight: 600;
  font-family: inherit;
  box-shadow: 0 2px 8px rgba(37, 99, 235, 0.2);
  transition: transform var(--transition-fast, 0.15s ease),
    box-shadow var(--transition-fast, 0.15s ease);
}

.dpo-log-btn:hover {
  transform: translateY(-1px);
  box-shadow: 0 4px 12px rgba(37, 99, 235, 0.3);
}

/*  Footer Actions Row  */
.dpo-footer-actions {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  gap: 8px;
  margin-top: 4px;
}

/*  Resolve/Unresolve Button  */
.dpo-resolve-btn {
  display: inline-flex;
  align-items: center;
  gap: 4px;
  background: linear-gradient(135deg, #16a34a, #15803d);
  color: white;
  border: none;
  padding: 6px 14px;
  border-radius: var(--radius-sm, 6px);
  cursor: pointer;
  font-size: 12px;
  font-weight: 600;
  font-family: inherit;
  box-shadow: 0 2px 8px rgba(22, 163, 74, 0.2);
  transition: transform 0.15s ease, box-shadow 0.15s ease, opacity 0.15s ease;
}

.dpo-resolve-btn:hover {
  transform: translateY(-1px);
  box-shadow: 0 4px 12px rgba(22, 163, 74, 0.3);
}

.dpo-resolve-btn:disabled {
  opacity: 0.6;
  cursor: not-allowed;
  transform: none;
}

.dpo-resolve-btn-undo {
  background: linear-gradient(135deg, #f59e0b, #d97706);
  box-shadow: 0 2px 8px rgba(245, 158, 11, 0.2);
}

.dpo-resolve-btn-undo:hover {
  box-shadow: 0 4px 12px rgba(245, 158, 11, 0.3);
}

/*  Resolved card state  */
.dpo-issue-card.dpo-issue-resolved {
  opacity: 0.7;
  border-left-color: #16a34a !important;
}

.dpo-resolved-badge {
  display: inline-flex;
  align-items: center;
  font-size: 0.65rem;
  font-weight: 700;
  color: #15803d;
  background: #dcfce7;
  padding: 2px 7px;
  border-radius: 4px;
  border: 1px solid #86efac;
  white-space: nowrap;
  text-transform: uppercase;
  letter-spacing: 0.04em;
}

/* =========================
   LOG MODAL
========================= */

.dpo-log-overlay {
  position: fixed;
  inset: 0;
  z-index: 3000;
  animation: fadeIn 0.2s ease;
}

@keyframes fadeIn {
  from {
    opacity: 0;
  }

  to {
    opacity: 1;
  }
}

.dpo-log-backdrop {
  position: absolute;
  inset: 0;
  background: rgba(0, 0, 0, 0.35);
  backdrop-filter: blur(4px);
  -webkit-backdrop-filter: blur(4px);
}

.dpo-log-modal {
  position: relative;
  background: var(--color-surface, #fff);
  width: 550px;
  max-height: 80vh;
  overflow-y: auto;
  margin: 70px auto;
  padding: 24px;
  border-radius: var(--radius-lg, 14px);
  z-index: 3001;
  box-shadow: 0 20px 50px rgba(0, 0, 0, 0.15);
  animation: fadeInCard 0.25s ease forwards;
}

.dpo-log-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.dpo-log-close {
  background: none;
  border: none;
  font-size: 18px;
  cursor: pointer;
  color: var(--color-text-muted, #9ca3af);
  transition: color var(--transition-fast, 0.15s ease);
}

.dpo-log-close:hover {
  color: var(--color-text, #1f2937);
}

.dpo-log-entry {
  display: flex;
  gap: 12px;
  margin-bottom: 15px;
}

.dpo-log-dot {
  width: 10px;
  height: 10px;
  background: var(--color-primary, #2563eb);
  border-radius: 50%;
  margin-top: 6px;
  flex-shrink: 0;
}

.dpo-log-msg {
  background: #f3f4f6;
  padding: 8px 12px;
  border-radius: var(--radius-md, 10px);
  flex: 1;
}

.dpo-log-date {
  font-size: 0.75rem;
  color: var(--color-text-muted, #9ca3af);
  margin-top: 4px;
}

/* =========================
   ANIMATION
========================= */

@keyframes fadeInCard {
  from {
    opacity: 0;
    transform: translateY(6px);
  }

  to {
    opacity: 1;
    transform: translateY(0);
  }
}
