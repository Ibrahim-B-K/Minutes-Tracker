/* ‚îÄ‚îÄ Download ‚îÄ‚îÄ */
.dpo-download-section {
  display: flex;
  align-items: center;
  justify-content: flex-end;
  height: 4px;
  margin-top: 25px;
  margin-right: 10px;
}

.dpo-generate-download-btn {
  background: linear-gradient(135deg, var(--color-primary, #2563eb), var(--color-primary-dark, #1d4ed8));
  color: white;
  border: none;
  padding: 10px 24px;
  border-radius: var(--radius-md, 10px);
  font-weight: 600;
  font-family: inherit;
